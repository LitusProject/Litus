{% for flashMessage in flashMessenger %}
	<div class="flashmessage {{ flashMessage.getType() }} {% if flashMessage.getFullWidth() %} full_width {% endif %}">
		<div class="title">{{ flashMessage.getTitle() }}</div>
		<div class="content">
			<p>{{ flashMessage.getMessage() }}</p>
		</div>
	</div>
{% endfor %}